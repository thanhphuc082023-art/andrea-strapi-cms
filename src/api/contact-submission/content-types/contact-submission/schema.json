{
  "kind": "collectionType",
  "collectionName": "contact_submissions",
  "info": {
    "singularName": "contact-submission",
    "pluralName": "contact-submissions",
    "displayName": "Contact Submission"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "industry": {
      "type": "string",
      "required": true
    },
    "message": {
      "type": "text",
      "required": true
    },
    "submittedAt": {
      "type": "datetime",
      "required": true
    },
    "ipAddress": {
      "type": "string"
    },
    "userAgent": {
      "type": "string"
    },
    "userStatus": {
      "type": "enumeration",
      "required": false,
      "default": "new",
      "enum": [
        "new",
        "contacted",
        "resolved"
      ]
    }
  }
}
